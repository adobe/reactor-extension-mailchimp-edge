{
  "name": "@adobe/reactor-extension-mailchimp-edge",
  "version": "1.0.2",
  "description": "Adobe event forwarding extension for Mailchimp",
  "scripts": {
    "build": "rm -rf dist && webpack --mode production",
    "build:watch": "webpack --mode development --watch",
    "sandbox": "concurrently -k -n webpack,sandbox -c \"bgBlue,bgMagenta\" \"npm run build:watch\" \"reactor-sandbox\"",
    "package": "npm run build && npx @adobe/reactor-packager",
    "prepack": "npm test && npm run build",
    "lint": "eslint \"src/**\"",
    "format": "pretty-quick",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "update": "rm -rf node_modules package-lock.json && npm i",
    "type": "module"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix"
    ]
  },
  "author": {
    "name": "Adobe",
    "url": "http://adobe.com",
    "email": "platform-support@adobe.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@adobe/reactor-sandbox": "12.2.0",
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "camelcase": "^6.3.0",
    "capitalize": "^2.0.4",
    "concurrently": "^7.0.0",
    "css-loader": "^6.5.1",
    "eslint": "8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.3",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.7",
    "jest-environment-jsdom": "^27.4.6",
    "jest-runner-eslint": "^1.0.0",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "style-loader": "^3.3.1",
    "stylus": "^0.56.0",
    "stylus-loader": "^6.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "yargs": "^17.3.1"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.15.1",
    "@spectrum-icons/illustrations": "^3.2.2",
    "@spectrum-icons/workflow": "^3.2.1",
    "classnames": "^2.3.1",
    "create-react-app": "^1.2.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.24.0"
  }
}
